{
    "id": "init",

    "server_tarball": "http://some-host/path/to/cloudos-server.tar.gz",
    "run_as": "cloudos",
    "admin_initial_pass": "bcrypted-initial-password-here",
    "recovery_email": "put-recovery-email-here",
    "aws_access_key": "aws_access_key",
    "aws_secret_key": "aws_secret_key",
    "aws_iam_user": "aws_iam_user",
    "s3_bucket": "s3_bucket",

    "authy": {
        "user": "authy-api-key",
        "base_uri": "http://sandbox-api.authy.com"
    },

    "dns": {
        "base_uri": "http://dns-manager-uri/cloudstead-will-fill-this-out-at-launch-time or omit if using DynDNS directly",
        "user": "cloudos-dns or dyn username goes here",
        "password": "cloudos-dns or dyn password goes here",
        "account": "if using Dyn, put main dyn account here. omit if using cloudos-dns",
        "zone": "if using Dyn, put zone name here. omit if using cloudos-dns"
    },

    "vendor": {
        "service_key_endpoint": "https://some-host/endpoint",
        "ssl_key_sha": "--some-sha256-sum--",
        "settings": [
            {
                "path": "cloudos.aws_access_key",
                "shasum": "--some-sha256-sum--"
            }, {
                "path": "cloudos.aws_secret_key",
                "shasum": "--some-sha256-sum--"
            }, {
                "path": "cloudos.aws_iam_user",
                "shasum": "--some-sha256-sum--"
            }, {
                "path": "cloudos.s3_bucket",
                "shasum": "--some-sha256-sum--"
            }, {
                "path": "cloudos.authy.user",
                "shasum": "--some-sha256-sum--"
            }
        ]
    }
}