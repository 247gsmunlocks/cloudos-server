<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<title>CloudOS administration</title>
	<link rel="stylesheet" href="css/app.css">
	<script src="js/app.js"></script>
	<script src="js/apps/admin-app.js"></script>
	<script src="localize?js/local/strings.js"></script>
</head>
<body>

<script type="text/x-handlebars">
	<section class="app-bar">
		<nav class="top-bar" data-topbar role="navigation">
			<ul class="title-area">
				<li class="name">
					<h1>
						<a class="brand" href="#">{{t app.title}}</a>
					</h1>
				</li>
				<li class="toggle-topbar menu-icon">
					<a href="#">
						<span></span>
					</a>
				</li>
			</ul>
			{{#if cloudos_account}}
				<section class="top-bar-section">
					<ul class="right">
						<li class="has-dropdown">
							<a href="#">
								<span class="icon-question-circle"><span class="caption-for-icon show-for-small">Help</span></span>
								<span class="show-for-xxlarge-up">Help</span>
							</a>
							<ul class="dropdown">
								<li>
									<a href=" http://www.cloudstead.io/blog/" target="_blank">{{t menu.help.news}}</a>
								</li>
								<li>
									<a href="http://cloudstead.uservoice.com/" target="_blank">{{t menu.help.ticket}}</a>
								</li>
							</ul>
						</li>
						{{#if cloudos_account.admin}}
							<li class="has-dropdown">
								<a href="#" data-dropdown="settings-submenu" data-options="align:left">
									<span class="icon-gear"><span class="caption-for-icon show-for-small">Settings</span></span>
									<span class="show-for-xxlarge-up">Settings</span>
								</a>
								<ul class="dropdown">
										<li><a href="/admin.html#/accounts">{{t menu.accounts}}</a></li>
										<li><a href="/admin.html#/email">{{t menu.emaildom}}</a></li>
										<li><a href="/admin.html#/security">{{t menu.securitycert}}</a></li>
									<li><a href="/admin.html#/appstore">{{t menu.appstore}}</a></li>
								</ul>
							</li>
						{{/if}}
						<li class="has-dropdown">
							<a href="#" data-dropdown="user-submenu" data-options="align:left">
								<span class="icon-user"><span class="caption-for-icon show-for-small">User</span></span>
								<span class="show-for-xxlarge-up">User</span>
							</a>
							<ul class="dropdown">
								<li><a href="/#/profile">{{t menu.profile}}</a>
								<li><a href="/#/settings">{{t menu.settings}}</a></li>
								<li>
									<a {{action 'select_app' 'settings'}}>
										{{t menu.mail_and_cal}}
									</a>
								</li>
								<li class="divider"></li>
								<li>{{#link-to 'logout'}}{{t sections.logout}}{{/link-to}}</li>
							</ul>
						</li>
					</ul>
					<ul class="nav-apps">
						{{#each app in cloudos_account.availableApps}}
							<li>
								<a {{action 'select_app' app.name}} {{bind-attr title=app.name}} class="toolbar_link">
									<img {{bind-attr src="app.assets.taskbarIconUrl"}} {{bind-attr alt=app.name}} class="toolbar_icon" />
									<span class="show-for-small">{{app-name app.name}}</span>
								</a>
							</li>
						{{/each}}
					</ul>
				</section>
				<section class="top-bar-section show-for-medium-up right">
					<div class="row">
						<div class="large-12 columns">
							<div class="row collapse">
								<div class="small-10 columns">
									<input type="text" placeholder="">
								</div>
								<div class="small-2 columns">
									<a href="#" class="button tiny" style="top:0px;"><span class="icon-search"></span> <span class="show-for-xxlarge-up">Search</span></a>
								</div>
							</div>
						</div>
					</div>
				</section>
			{{/if}}
		</nav>
	</section>

	{{outlet}}
</script>

	<script type="text/x-handlebars" id="login">
			<div class="container-fluid">
					<div class="row-fluid">
							<div class="span3">
									{{#if username}}
											<table class='table'>
													<thead>
													<tr><th>{{t sections.login}}</th></tr>
													</thead>
													<!--<tr>-->
															<!--<td>{{t forms.login.username}}</td>-->
															<!--<td>{{input type="text" value=username}}</td>-->
													<!--</tr>-->
													<tr>
															<td>{{t forms.login.password}}</td>
															<td>{{input type="password" value=password}}</td>
													</tr>
													<tr>
															<td></td><td><button {{action 'doLogin'}}>{{t forms.login.login_button}}</button></td>
													</tr>
													<tr><td></td><td><button {{action 'clearStorage'}}>clear storage</button></td></tr>
											</table>
									{{else}}
											<table class='table'>
													<thead>
													<tr><th colspan="2">{{t sections.new_admin_account}}</th></tr>
													</thead>
													<tr><td colspan="2"><p>{{t paragraphs.new_admin_account}}</p></td></tr>
													<!--<tr>-->
															<!--<td>{{t forms.login.username}}:</td>-->
															<!--<td>{{input type="text" value=username}}</td>-->
													<!--</tr>-->
													<tr>
															<td>{{t forms.login.password}}:</td>
															<td>{{input type="password" value=password}}</td>
													</tr>
													<tr>
															<td>{{t forms.login.password2}}:</td>
															<td>{{input type="password" value=password2}}</td>
													</tr>
													<tr>
															<td></td><td><button {{action 'doNewAccount'}}>{{t forms.login.new_admin_account_button}}</button></td>
													</tr>
													<tr><td></td><td><button {{action 'clearStorage'}}>clear storage</button></td></tr>
											</table>
									{{/if}}
							</div>
					</div>
			</div>
	</script>

	<script type="text/x-handlebars" id="apps">
		<div class="container-fluid">
			<div class="row-fluid">
				<div class="span3">
					<table class='table'>
						<thead>
							<tr><th>{{t sections.apps}}</th></tr>
						</thead>
						<!--{{#each model}}-->
						<!--<tr><td>-->
								<!--{{#link-to 'post' this}}{{title}} <small class='muted'>by {{author.name}}</small>{{/link-to}}-->
						<!--</td></tr>-->
						<!--{{/each}}-->
					</table>
				</div>
				<div class="span9">
					{{outlet}}
				</div>
			</div>
		</div>
	</script>

	<script type="text/x-handlebars" id="email">
			<section class="deck">
					<div class="row">
							<div class="medium-8 large-8 medium-centered columns">
								 <fieldset class="wrap-set">
											<ul>
													<li>{{#link-to 'email.domains'}}{{t sections.email.domains}}{{/link-to}}</li>
													<li>{{#link-to 'aliases'}}{{t sections.email.aliases}}{{/link-to}}</li>
											</ul>

							{{outlet}}
				</fieldset>
							</div>
					</div>
			</section>
	</script>

	<script type="text/x-handlebars" id="email/domains">
			<div class="row">
			<br /><br />
					<table class='table'>
							<thead>
							<tr><th>{{t sections.email.domains}}</th></tr>
							</thead>
							<tr>
									<td>
											{{t sections.email.mxrecord}}: {{model.mxrecord}}
											<hr/>
											{{t paragraphs.email.domains}}
											<hr/>
									</td>
							</tr>
							{{#each domain in model.domains}}
							<tr>
									<td> {{domain.name}} </td>
									<td><button {{action 'doRemoveDomain' domain.name}}>{{t forms.email.remove_domain}}</button></td>
							</tr>
							{{/each}}
					</table>
					<hr/>
					<table class='table'>
							<thead>
							<tr><th colspan="2">{{t sections.add_domain}}</th></tr>
							</thead>
							<tr>
									<td>{{t forms.email.domain_to_add}}:</td>
									<td>{{input type="text" value=domain}}</td>
							</tr>
							<tr>
									<td colspan="2"><button {{action 'doAddDomain'}}>{{t forms.email.add_domain}}</button></td>
							</tr>
					</table>
			</div>
	</script>

	<script type="text/x-handlebars" id="aliases/index">
		<br /><br />
			<div class="row">
					<table >
							<thead>
							<tr><th>{{t sections.email.aliases}}</th></tr>
							</thead>
							<tbody>
							{{#each model}}
							<tr>
							<tr>
									<td>{{#link-to 'alias' name}}{{name}}{{/link-to}}</td>
									<td>{{recipients}}</td>
									<td><button {{action 'doRemoveAlias' name}}>{{t forms.email.alias.delete_button}}</button></td>
							</tr>
							{{/each}}
							<tr>
									<td>
											{{#link-to 'aliases.new'}}{{t forms.email.alias.add_button}}{{/link-to}}
									</td>
							</tr>
							</tbody>
					</table>
					<hr/>
			</div>
	</script>

	<script type="text/x-handlebars" id="aliases/new">
		<div class="row">
			<br /><br />
					<table >
							<thead>
							<tr><th colspan="2">{{t sections.add_alias}}</th></tr>
							</thead>
							<tr>
									<td>{{t forms.email.alias.name}}:</td>
									<td>{{input type="text" value=aliasName}}</td>
							</tr>
							<tr>
									<td>{{t forms.email.alias.recipients}}:</td>
									<td>{{input type="text" value=recipients}}</td>
							</tr>
							<tr>
									<td colspan="2"><button {{action 'doAddAlias'}}>{{t forms.email.alias.add_button}}</button></td>
							</tr>
					</table>
			</div>
	</script>

	<script type="text/x-handlebars" id="alias">
		<div class="row">
			<br /><br />
					<table class='table'>
							<thead>
							<tr><th colspan="2">{{t sections.edit_alias}}</th></tr>
							</thead>
							<tr>
									<td>{{t forms.email.alias.name}}:</td>
									<td>{{input type="text" value=aliasName}}</td>
							</tr>
							<tr>
									<td>{{t forms.email.alias.recipients}}:</td>
									<td>{{input type="text" value=recipients}}</td>
							</tr>
							<tr>
									<td colspan="2"><button {{action 'doEditAlias'}}>{{t forms.email.alias.edit_button}}</button></td>
							</tr>
					</table>
			</div>
	</script>

	<script type="text/x-handlebars" id="certs/index">
		<section class="deck">
			<div class="medium-10 large-10 medium-centered columns">
					<table>
							<thead>
							<tr><th colspan="6">{{t sections.security.certs}}</th></tr>
							</thead>
							<tbody>
							<tr>
									<th>Name</th>
									<th>Description</th>
									<th>Common name</th>
									<th>PEM</th>
									<th>Key</th>
					<th></th>
							</tr>
							{{#each model}}
							<tr>
									<td>{{#link-to 'cert' name}}{{name}}{{/link-to}}</td>
									<td>{{description}}</td>
									<td>{{commonName}}</td>
									<td><small>sha256: {{pemSha}}<br/>md5: {{pemMd5}}</small></td>
									<td><small>sha256: {{keySha}}<br/>md5: {{keyMd5}}</small></td>
									<td><button {{action 'doRemoveCert' name}}>{{t forms.security.certs.delete_button}}</button></td>
							</tr>
							{{/each}}
							<tr>
									<td colspan="6">
											{{#link-to 'certs.new'}}{{t forms.security.certs.add_button}}{{/link-to}}
									</td>
							</tr>
							</tbody>
					</table>
			</div>
		</section>
	</script>

	<script type="text/x-handlebars" id="certs/new">
		<section class="deck">
			<div class="row">
				<div class="medium-6 large-5 medium-centered columns">
					<form {{action 'doAddCert' on="submit"}}>
						<fieldset class="wrap-set">
							<legend>{{t sections.add_cert}}</legend>
							<div class="field-container">
								<label for="text">{{t forms.security.certs.name}}</label>
								{{#if requestMessages.error.cert_name}}
									{{input type="text" value=certName class="error"}}
								{{else}}
									{{input type="text" value=certName}}
								{{/if}}

								{{#if requestMessages.error.cert_name}}
									<span class="message error">{{requestMessages.error.cert_name}}</span>
								{{/if}}
							</div>
							<div class="field-container">
								<label for="text">{{t forms.security.certs.description}}</label>
								{{#if requestMessages.error.cert_description}}
									{{input type="text" value=description class="error"}}
								{{else}}
									{{input type="text" value=description}}
								{{/if}}

								{{#if requestMessages.error.cert_description}}
									<span class="message error">{{requestMessages.error.cert_description}}</span>
								{{/if}}
							</div>
							<div class="field-container">
								<label for="text">{{t forms.security.certs.pem}}</label>
								{{#if requestMessages.error.cert_pem}}
									{{textarea value=pem class="error"}}
								{{else}}
									{{textarea value=pem}}
								{{/if}}

								{{#if requestMessages.error.cert_pem}}
									<span class="message error">{{requestMessages.error.cert_pem}}</span>
								{{/if}}
							</div>
							<div class="field-container">
								<label for="text">{{t forms.security.certs.key}}</label>
								{{#if requestMessages.error.cert_key}}
									{{textarea value=key class="error"}}
								{{else}}
									{{textarea value=key}}
								{{/if}}

								{{#if requestMessages.error.cert_key}}
									<span class="message error">{{requestMessages.error.cert_key}}</span>
								{{/if}}
							</div>
							<div class="field-container">
								<label>&nbsp;</label>
								<p><button type="submit" class="expand" {{action 'doAddCert'}}>{{t forms.security.certs.add_button }}</button></p>
							</div>
						</fieldset>
					</form>
				</div>
			</div>
		</section>
	</script>

	<script type="text/x-handlebars" id="cert">
		<section class="deck">
			<div class="row">
				<div class="medium-8 large-8 medium-centered columns">
					<form {{action 'doAddCert' on="submit"}}>
						<fieldset class="wrap-set">
							<legend>{{t sections.overwrite_cert}}</legend>
							<div class="field-container">
								<label for="text">{{t forms.security.certs.name}}</label>
								{{#if requestMessages.error.cert_name}}
									{{input type="text" value=certName class="error"}}
								{{else}}
									{{input type="text" value=certName}}
								{{/if}}

								{{#if requestMessages.error.cert_name}}
									<span class="message error">{{requestMessages.error.cert_name}}</span>
								{{/if}}
							</div>
							<div class="field-container">
								<label for="text">{{t forms.security.certs.description}}</label>
								{{#if requestMessages.error.cert_description}}
									{{input type="text" value=description class="error"}}
								{{else}}
									{{input type="text" value=description}}
								{{/if}}

								{{#if requestMessages.error.cert_description}}
									<span class="message error">{{requestMessages.error.cert_description}}</span>
								{{/if}}
							</div>
							<div class="field-container">
								<label for="text">{{t forms.security.certs.pem}}</label>
								<small>sha256: {{pemSha}}<br/>md5: {{pemMd5}}<br/></small>
								{{#if requestMessages.error.cert_pem}}
									{{textarea value=pem class="error"}}
								{{else}}
									{{textarea value=pem}}
								{{/if}}

								{{#if requestMessages.error.cert_pem}}
									<span class="message error">{{requestMessages.error.cert_pem}}</span>
								{{/if}}
							</div>
							<div class="field-container">
								<label for="text">{{t forms.security.certs.key}}</label>
								<small>sha256: {{keySha}}<br/>md5: {{keyMd5}}<br/></small>
								{{#if requestMessages.error.cert_key}}
									{{textarea value=key class="error"}}
								{{else}}
									{{textarea value=key}}
								{{/if}}

								{{#if requestMessages.error.cert_key}}
									<span class="message error">{{requestMessages.error.cert_key}}</span>
								{{/if}}
							</div>
							<div class="field-container">
								<label>&nbsp;</label>
								<p><button type="submit" class="expand" {{action 'doAddCert'}}>{{t forms.security.certs.overwrite_button}}</button></p>
							</div>
						</fieldset>
					</form>
				</div>
			</div>
		<section>
	</script>

<script type="text/x-handlebars" id="selectedapp">
	<section class="deck">
		<div class="row">
			<dl class="tabs" data-tab>
				<dd class="active half-width-tab">
					{{#link-to 'appstore'}}<span class="icon-mail-reply"></span>{{/link-to}}
				</dd>
				<dd class="half-width-tab">
					{{#link-to 'installedapps'}}{{t sections.appstr.installed_apps}}{{/link-to}}
				</dd>
			</dl>
			<div class="tabs-content">
				<div class="content active">
					<div class="row event-single no-margin">
						<div class="large-2 columns">
							<h4>{{name}} App</h4>single
						</div>
						<div class="large-4 columns">
							<p>by {{publisher.name}}</p>
						</div>
						<div class="large-4 columns rating">
							<p><span class="icon-star"></span><span class="icon-star"></span><span class="icon-star"></span><span class="icon-star"></span><span class="icon-star"></span></p>
							<p>12,340 Cloudstead Users</p>
						</div>
						<div class="large-2 columns">
							{{#if isInstalled}}
								<button class="tiny secondary expand" {{action 'select_app' name}}>Open</button>
							{{else}}
								<button class="tiny primary expand" {{action 'install_app' name}}>install</button>
							{{/if}}
						</div>
					</div>
					<div class="row">
						&nbsp;
					</div>
					<div class="row">
						<div class="large-4 columns">
							<div class="circle"><img {{bind-attr src=smallIcon}}></div>
						</div>
						<div class="large-8 columns small-only-text-center">
							<div class="large-12 columns">
								<p>Overview : </p>
								<p>{{description}}</p>
							</div>
						</div>
					</div>
					<div class="row">
						&nbsp;
					</div>
					<div class="row event-single no-margin">
						<div class="large-12 columns text-center rating">
							<h4>RATE THIS APP</h4>
							<p><span class="icon-star-o"><span class="icon-star-o"><span class="icon-star-o"><span class="icon-star-o"><span class="icon-star-o"></p>
							<p>Install and Provide an App Rating</p>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>
</script>

<script type="text/x-handlebars" id="installedapps">
	<section class="deck">
		<div class="row">
			<dl class="tabs" data-tab>
				<dd class="half-width-tab">
					{{#link-to 'appstore'}}{{t sections.appstr.available_apps}}{{/link-to}}
				</dd>
				<dd class="active half-width-tab">
					{{#link-to 'installedapps'}}{{t sections.appstr.installed_apps}}{{/link-to}}
				</dd>
			</dl>
			<div class="tabs-content">
				<div class="content active">
					<div class="large-12 columns no-padd">
					<table style="width: 100%">
						<thead>
							<th>
								App Name <span class="icon-sort-down table-icon"></span>
								<span class="icon-sort-up table-icon table-icon-upper"></span>
							</th>
							<th>
								Version <span class="icon-sort-down table-icon"></span>
								<span class="icon-sort-up table-icon table-icon-upper"></span>
							</th>
							<th>
								Maintainer <span class="icon-sort-down table-icon"></span>
								<span class="icon-sort-up table-icon table-icon-upper"></span>
							</th>
							<th>
								Installed By <span class="icon-sort-down table-icon"></span>
								<span class="icon-sort-up table-icon table-icon-upper"></span>
							</th>
						</thead>
						<tbody>
							{{#each app in arrangedContent itemController='installedapp'}}
								<tr>
									<td>{{app.manifest.name}}</td>
									<td>{{app.manifest.version}}</td>
									<td>
										{{app.manifest.publisher.maintainer}}
										{{app.manifest.publisher.maintainer_email}}
									</td>
									<td>
										{{app.installedBy}}
									</td>
								</tr>
							{{/each}}
						</tbody>
					</table>
				</div>
				</div>
			</div>
		</div>
	</section>
</script>

<script type="text/x-handlebars" id="appstore">
	<section class="deck">
		<div class="row">
			<dl class="tabs" data-tab>
				<dd class="active half-width-tab">
					{{#link-to 'appstore'}}{{t sections.appstr.available_apps}}{{/link-to}}
				</dd>
				<dd class="half-width-tab">
					{{#link-to 'installedapps'}}{{t sections.appstr.installed_apps}}{{/link-to}}
				</dd>
			</dl>
			<div class="tabs-content">
				<div class="content active">
					<div class="circles row text-center">
						{{#each app in arrangedContent}}
							<div class="large-4 medium-6 columns">
								{{#if app.isInstalled}}
									<div class="icon-check  success-check installed-app-check"></div>
								{{/if}}
								{{#link-to 'selectedapp' app.name}}
									<div class="circle"><img {{bind-attr src=app.smallIcon}} alt=""/></div>
									<h3 class="gamma light">{{app.name}}</h3>
									<p>{{app.description}}</p>
								{{/link-to}}
							</div>
						{{/each}}
					</div>
					{{#if morePagesAvailable}}
						<a {{action 'loadNextPage'}}>More Apps</a>
					{{/if}}
				</div>
			</div>
		</div>
	</section>
</script>

<script type="text/x-handlebars" id="tasks">
		<div class="container-fluid">
				<div class="row-fluid">
						<div class="span9">
								<table class='table'>
										<thead>
										<tr><th>{{task-description model.result}}</th></tr>
										</thead>
										<tbody>
										{{#if model.result.success}}
										<tr>
												<td>success!</td>
												<td>{{model.result.success}}</td>
