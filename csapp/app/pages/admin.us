						<li><a class="button tiny alert">{{t sections.acct.groups}}</a></li>
						<li><a href="#" class="button tiny">{{t sections.acct.permissions}}</a></li>
					</ul>
					<ul class="inline-list accounts-list">
						<li>
							{{#link-to 'groups.new' data-dropdown="" data-options=""}}
								<span class="icon-plus" title="Add Group" ></span> <span class="show-for-xxlarge-up"></span>
							{{/link-to}}
						</li>
					</ul>
				</div>
				<div class="large-12 columns no-padd">
					<div class="large-2 columns no-padd">
						<select class="select-small">
							<option value="edit">{{t sections.acct.bulk.edit}}</option>
							<option value="toggle_status">{{t sections.acct.bulk.toggle_status}}</option>
							<option value="delete">{{t sections.acct.bulk.delete}}</option>
						</select>
					</div>
					<div class="large-2 columns no-padd">
						<a href="#" class="button tiny">Apply</a>
					</div>
					<div class="large-8 columns no-padd">
					</div>
				</div>
				<div class="large-12 columns no-padd">
					<table style="width: 100%">
						<thead>
							<th></th>
							<th>
								{{t sections.acct.table.group_name}}
								<span class="icon-sort-down table-icon"></span>
								<span class="icon-sort-up table-icon table-icon-upper"></span>
							</th>
							<th>
								{{t sections.acct.table.description}}
								<span class="icon-sort-down table-icon"></span>
								<span class="icon-sort-up table-icon table-icon-upper"></span>
							</th>
							<th>
								{{t sections.acct.table.group_email_address}}
								<span class="icon-sort-down table-icon"></span>
								<span class="icon-sort-up table-icon table-icon-upper"></span>
							</th>
							<th>
								{{t sections.acct.table.group_admin}}
								<span class="icon-sort-down table-icon"></span>
								<span class="icon-sort-up table-icon table-icon-upper"></span>
							</th>
							<th>
								{{t sections.acct.table.member_count}}
								<span class="icon-sort-down table-icon"></span>
								<span class="icon-sort-up table-icon table-icon-upper"></span>
							</th>
							<th>
								{{t sections.acct.table.status}}
								<span class="icon-sort-down table-icon"></span>
								<span class="icon-sort-up table-icon table-icon-upper"></span>
							</th>
							<th>
								{{t sections.acct.table.file_storage_used}}
								<span class="icon-sort-down table-icon"></span>
								<span class="icon-sort-up table-icon table-icon-upper"></span>
							</th>
							<th>
								{{t sections.acct.table.total_storage_available}}
								<span class="icon-sort-down table-icon"></span>
								<span class="icon-sort-up table-icon table-icon-upper"></span>
							</th>
							<th>
								<span class="icon-sort-down right"></span>
								<span class="icon-sort-up table-icon table-icon-upper right" style="margin-right: -6px;"></span>
							</th>
						</thead>
						<tbody>
							{{#each sortedGroups}}
								<tr>
									<td>{{input type="checkbox"}}</td>
									<td>{{#link-to "group" name}}{{name}}{{/link-to}}</td>
									<td></td>
									<td></td>
									<td></td>
									<td></td>
									<td></td>
									<td></td>
									<td></td>
									<td><a {{action "doDeleteGroup" name}}>{{t delete_button}}</a></td>
								</tr>
							{{/each}}
						</tbody>
					</table>
				</div>
			</div>
		</div>
		</div>
		{{outlet group_outlet}}
	</section>
</script>

<script type="text/x-handlebars" id="groups/new">
	{{#modal-dialog}}
		<div class="row modal-filler">
			<legend>{{t sections.add_group}}</legend>
		</div>
		<div class="row modal-content">
			<section class="deck">
				<div class="row">
					<label>{{t forms.group.name}}
						{{input type="text" value=name}}
					</label>
				</div>
				<div class="row">
					<label>{{t forms.group.recipients}}
						{{input type="text" value=recipients}}
					</label>
				</div>
				<div class="row">
					<label>{{t forms.group.quota}}
						{{input type="text" value=storageQuota}}
					</label>
				</div>
				<div class="row">
					<label>{{t forms.group.description}}
						{{input type="text" value=description}}
					</label>
				</div>
				<div class="row">
					<div class="large-6 columns">
						<button {{action 'doCancel'}}>
							{{t forms.group.cancel}}
						</button>
					</div>
					<div class="large-6 columns">
						<button {{action 'doAddGroup'}}>
							{{t forms.group.add_button}}
						</button>
					</div>
				</div>
			</section>
		</div>
	{{/modal-dialog}}
</script>

<script type="text/x-handlebars" id="group">
	{{#modal-dialog}}
		<div class="row modal-filler">
			<legend>{{t sections.add_group}}</legend>
		</div>
		<div class="row modal-content">
			<section class="deck">
				<div class="row">
					<label>{{t forms.group.name}}
						{{input type="text" value=name}}
					</label>
				</div>
				<div class="row">
					<label>{{t forms.group.recipients}}
						{{input type="text" value=recipients}}
					</label>
				</div>
				<div class="row">
					<div class="large-6 columns">
						<button {{action 'doCancel'}}>
							{{t forms.group.cancel}}
						</button>
					</div>
					<div class="large-6 columns">
						<button {{action 'doEditGroup'}}>
							{{t forms.group.edit_button}}
						</button>
					</div>
				</div>
			</section>
		</div>
	{{/modal-dialog}}
</script>

<script type="text/x-handlebars" id="components/modal-dialog">
	<div class="overlay" {{action "doCancel"}}>
	</div>
	<div class="modal">
		{{yield}}
	</div>
</script>

</body>
</html>
